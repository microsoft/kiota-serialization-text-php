{
    "name": "microsoft/kiota-serialization-text",
    "description": "Implementation of Serialization Abstractions for text/plain content",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Microsoft Graph Client Tooling",
            "email": "graphtooling@service.microsoft.com"
        }
    ],
    "require": {
		"php": "^7.4 | ^8.0",
		"microsoft/kiota-abstractions": "^1.3.1",
		"guzzlehttp/psr7": "^1.6 || ^2"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.5",
		"phpstan/phpstan": "^1.2 || ^2.0"
	},
	"autoload": {
		"psr-4": {
			"Microsoft\\Kiota\\Serialization\\Text\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Microsoft\\Kiota\\Serialization\\Text\\Tests\\": "tests/"
		}
	},
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
